<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="adminhtml.customer.group.grid.columnSet">
            <block class="Magento\Backend\Block\Widget\Grid\Column"
                   name="adminhtml.customer.group.grid.columnSet.tango_id" as="tango_id"
                   after="adminhtml.customer.group.grid.columnSet.class_name">
                <arguments>
                    <argument name="header" xsi:type="string" translate="true">Tango ID</argument>
                    <argument name="index" xsi:type="string">tango_id</argument>
                    <argument name="default" xsi:type="string">*</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
